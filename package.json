		"@types/react-dom": "18.2.23",
		"autoprefixer": "10.4.19",
		"concurrently": "^8.2.2",
		"eslint": "8.57.0",
		"eslint-config-next": "14.1.4",
		"next": "14.1.4",
		"postcss": "8.4.38",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"tailwind": "^4.0.0",
		"tailwindcss": "3.4.3",
		"typescript": "5.4.3",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@babel/core": "^7.24.3",
